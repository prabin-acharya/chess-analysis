{
  "manifest_version": 3,
  "name": "Chess Analysis",
  "description": "",
  "version": "0.1",
  "permissions": ["activeTab", "webNavigation"],
  "content_scripts": [
    {
      "matches": ["*://*.chess.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon/knight.png"
  }
}
